<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>

<VBox fx:controller="controller.SessionController" styleClass="background" prefHeight="320.0" prefWidth="600.0" spacing="10" alignment="TOP_CENTER" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fitHeight="160" fitWidth="600" preserveRatio="true"><image><Image url="@loGo.png"/></image></ImageView>
    <Region prefHeight="70"/>
    <Text styleClass="text-type" text="Membership Management System"/>
    <HBox alignment="CENTER" spacing="30">
        <Button fx:id="loginBtn" text="Login" onAction="#handleLogin"/>
        <Button fx:id="exitBtn" text="Exit" onAction="#handleExit"/>
    </HBox>
    <stylesheets>
        <URL value="@mmsfx.css"/>
    </stylesheets>
</VBox>
